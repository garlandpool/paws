<p id="notice"><%= notice %></p>

<h1>Listing Artists</h1>

<table>
  <thead>
    <tr>
      <th>First</th>
      <th>Last</th>
      <th>Title</th>
      <th>Email</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Bday</th>
      <th>Bmonth</th>
      <th>Byear</th>
      <th>Homephone</th>
      <th>Cellphone</th>
      <th>Officephone</th>
      <th>Zipareas</th>
      <th>Contract day</th>
      <th>Contract month</th>
      <th>Contract year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr>
        <td><%= artist.first %></td>
        <td><%= artist.last %></td>
        <td><%= artist.title %></td>
        <td><%= artist.email %></td>
        <td><%= artist.address %></td>
        <td><%= artist.city %></td>
        <td><%= artist.state %></td>
        <td><%= artist.zipcode %></td>
        <td><%= artist.bday %></td>
        <td><%= artist.bmonth %></td>
        <td><%= artist.byear %></td>
        <td><%= artist.homephone %></td>
        <td><%= artist.cellphone %></td>
        <td><%= artist.officephone %></td>
        <td><%= artist.zipareas %></td>
        <td><%= artist.contract_day %></td>
        <td><%= artist.contract_month %></td>
        <td><%= artist.contract_year %></td>
        <td><%= link_to 'Show', artist %></td>
        <td><%= link_to 'Edit', edit_artist_path(artist) %></td>
        <td><%= link_to 'Destroy', artist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artist', new_artist_path %>
