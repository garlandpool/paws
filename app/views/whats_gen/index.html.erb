<p id="notice"><%= notice %></p>

<h1>Listing Whats</h1>

<table>
  <thead>
    <tr>
      <th>Font awesome</th>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @whats.each do |what| %>
      <tr>
        <td><%= what.font_awesome %></td>
        <td><%= what.title %></td>
        <td><%= what.body %></td>
        <td><%= link_to 'Show', what %></td>
        <td><%= link_to 'Edit', edit_what_path(what) %></td>
        <td><%= link_to 'Destroy', what, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New What', new_what_path %>
